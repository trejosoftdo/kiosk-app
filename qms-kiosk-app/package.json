    "i18next": "^23.5.1",
    "jest": "^29.2.1",
    "jest-expo": "^49.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.2.2",
    "react-native": "0.72.4",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-i18n": "^2.0.15",
    "react-native-paper": "^5.10.6",
    "react-native-safe-area-context": "4.6.3",
